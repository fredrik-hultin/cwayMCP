# Cway MCP Server Environment Variables
# Copy this file to .env and fill in your actual values

# Cway API Configuration (Required)
CWAY_API_TOKEN=your_bearer_token_here

# Cway API URL (Optional - defaults to https://app.cway.se/graphql)
# CWAY_API_URL=https://app.cway.se/graphql

# MCP Server Configuration (Optional)
# MCP_SERVER_HOST=localhost
# MCP_SERVER_PORT=8000

# Development Configuration (Optional)
# LOG_LEVEL=INFO
# DEBUG=false

# Request Configuration (Optional)
# REQUEST_TIMEOUT=30
# MAX_RETRIES=3